#import "ModelSyncBase.h"

/**
 *  应用类型
 */
typedef NS_ENUM(NSInteger, AppType) {
    /**
     *  网页应用
     */
    AppTypeWeb      = 1,
    /**
     *  本地原生App
     */
    AppTypeNative   = 2,
    /**
     *  浏览器内置功能
     */
    AppTypeFunc     = 3
};

/**
 *  应用
 */
@interface ModelApp : ModelSyncBase

/**
 *  App类型
 */
@property (nonatomic, assign) AppType appType;
/**
 *  图标
 */
@property (nonatomic, strong) NSString *icon;
/**
 *  名称
 */
@property (nonatomic, strong) NSString *name;
/**
 *  网页链接地址/浏览器内置功能模块的地址
 */
@property (nonatomic, strong) NSString *link;
/**
 *  iOS 原生App 的URL Schemes
 */
@property (nonatomic, strong) NSString *urlSchemes;
/**
 *  App 下载链接地址
 */
@property (nonatomic, strong) NSString *downloadLink;
/**
 *  排序编号 >= 0
 */
@property (nonatomic, assign) NSInteger orderIndex;

@end
